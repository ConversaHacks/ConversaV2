# Conversa Environment Variables

# Gemini API Key (required for validation)
GEMINI_API_KEY="sk-your-api-key"

# Gemini API Base URL (default: localhost:3030 proxy)
GEMINI_BASE_URL="http://localhost:3030"

# Gemini Model (optional)
GEMINI_MODEL="gemini-2.0-flash"

# Use proxy mode with OpenAI chat completions format (default: true)
GEMINI_USE_PROXY="true"
